<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zzw.domain.StudentInfo" table="student_info" schema="studentinfomanager">
        <id name="stu_id">
            <generator class="native"/>
        </id>
        <property name="stu_num" />
        <property name="stu_name" />
        <property name="stu_gender"/>
        <property name="stu_age" />
        <property name="stu_chengji" />
        <property name="stu_class" />
        <property name="stu_teacher" />
    </class>
</hibernate-mapping>