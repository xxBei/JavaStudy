<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zzw.domain.TeacherInfo" table="teacher_info" schema="studentinfomanager">
        <id name="th_id" column="th_id"/>
        <property name="th_name" />
        <property name="th_gender" />
        <property name="th_username" />
        <property name="th_password" />
        <property name="th_class" />
    </class>

</hibernate-mapping>