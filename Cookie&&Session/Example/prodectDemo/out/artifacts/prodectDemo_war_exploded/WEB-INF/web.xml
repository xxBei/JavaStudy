<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>product_info</servlet-name>
        <servlet-class>com.zzw.product.product_info</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>product_info</servlet-name>
        <url-pattern>/product_info</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>clearHistory</servlet-name>
        <servlet-class>com.zzw.product.clearHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clearHistory</servlet-name>
        <url-pattern>/clearHistory</url-pattern>
    </servlet-mapping>
</web-app>